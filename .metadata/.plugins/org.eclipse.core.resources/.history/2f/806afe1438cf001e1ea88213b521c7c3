package com.in28days.loops;

public class WhileLoops {
	private int num;

	WhileLoops(int num) {
		this.num = num;
	}

	void printSquaresUpToLimit() {
		int i = 1;
		while (i < num) {
			System.out.println(i * i);
		}
	}

	void printCubessUpToLimit() {
		int i = 1;
		while (i < num) {
			System.out.println(i * i * i);
		}
	}

}
