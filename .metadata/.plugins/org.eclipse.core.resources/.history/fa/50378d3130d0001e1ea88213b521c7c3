package com.in28minutes.oopAgain.level4.interfaces;

public class GameRunner {

	public static void main(String[] args) {
		MarioGame game = new MarioGame();

		game.up();
		game.down();
		game.left();
		game.right();

		ChessGame cg = new ChessGame();

		cg.up();
		cg.down();
		cg.left();
		cg.right();

	}

}
