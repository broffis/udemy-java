package com.in28minutes.arrays;

public class Student {
	private int[] marks;

	Student(int[] marks) {
		this.marks = marks;
	}

	int getNumberOfMarks() {
		return marks.length;
	}

	int getTotalSumOfMarks() {
		int sum = 0;

		for (int i = 0; i < marks.length - 1; i++) {
			sum += marks[i];
		}

		return sum;
	}

	int getMaximumMark() {
	}

	int getMinimumMark() {
	};

	int getAverageMarks() {
	};

}
