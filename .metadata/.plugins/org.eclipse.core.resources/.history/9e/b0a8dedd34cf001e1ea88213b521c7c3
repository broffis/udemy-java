package com.in28days.loops;

public class LoopRunner {

	public static void main(String[] args) {
		forLoop();

		MyNumber number = new MyNumber(9);
		
		boolean isPrime = number.isPrime();
		
		System.out.println("isPrime " + isPrime);
	}

	static void forLoop() {
		for (int i = 0; i < 10; i++) {
			System.out.print(i + " ");
		}
	}

}
