package com.in28minutes.oopAgain;

public class Fan {
	private String make;
	private double radius;
	private String color;
	private boolean isOn;
	private int speed;
	
	Fan(String make, double radius, String color) {
		this.make = make;
		this.radius = radius;
		this.color = color;
		this.isOn = false;
		this.speed = 0;
	}
}
